<settings>
    <servers>
        <server>
            <id>ossrh</id>
            <username>${env.SONATYPE_USER}</username>
            <password>${env.SONATYPE_PASSWORD}</password>

        </server>
    </servers>

    <profiles>
        <profile>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <gpg.executable>gpg2</gpg.executable>
            </properties>
        </profile>
    </profiles>
</settings>
